    "postinstall": "scripts/postinstall.sh" 
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^10.1.7",
		"@types/node": "^17.0.21",
		"blurhash": "^2.0.5",
		"typescript": "^5.1.6"
	}
}
